{% extends '::base_vanilla.html.twig' %}

{% block html_class %}<html class="bg-black">{% endblock %}

{% block meta %}
    {{ parent() }}
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>
{% endblock %}

{% block title %}Identification{% endblock %}

{% block stylesheets %}
    <!-- bootstrap 3.0.2 -->
    <link href="{{ asset('bundles/admin/css/bootstrap.min.css') }}" rel="stylesheet" type="text/css" />
    <!-- Theme style -->
    <link href="{{ asset('bundles/admin/css/AdminLTE.css') }}" rel="stylesheet" type="text/css" />
    <!-- Feuille de style custom pour l'api -->
    <link href="{{ asset('bundles/admin/css/custom_baseapi.css') }}" rel="stylesheet" type="text/css" />
{% endblock %}

{% block javascripts_head %}
